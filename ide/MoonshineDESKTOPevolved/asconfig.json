{
	"compilerOptions": {
		"output": "bin-debug/MoonshineDESKTOP.swf",
		"accessible": true,
		"swf-version": 39,
		"source-path": [
			"src",
			"../MoonshineSharedCore/src",
			"../MoonshineSharedCore/src/locale/en_US",
			"../../../Moonshine-SDK-Installer/InstallerSharedCore/src",
			"../../../VisualEditorConverterLib/src",
			"../../../MockupVisualEditor/src"
		],
		"library-path": [
			"libs",
			"libs/ApacheFlexSDKInstallerLib.swc",
			"../MoonshineSharedCore/libs",
			"../MoonshineGUICore/bin/flash/bin/MoonshineGUICore.swc"
		],
		"define": [
			{
				"name": "CONFIG::MOONSHINE",
				"value": true
			},
			{
				"name": "CONFIG::OSX",
				"value": false
			}
		]
	},
	"files": [
		"src/MoonshineDESKTOP.mxml"
	],
	"application": "src/MoonshineDESKTOP-app.xml",
	"copySourcePathAssets": true,
	"additionalOptions": "-locale en_US ja_JP -allow-source-path-overlap=true",
	"airOptions": {
		"output": "MoonshineDESKTOP.air",
		"extdir": [
			"extensions/SSB.ane"
		],
		"files": [
			{
				"file": "tourDeFlex",
				"path": "tourDeFlex"
			},
			{
				"file": "../MoonshineSharedCore/src/elements",
				"path": "elements"
			}
		]
	},
	"config": "air"
}